            topWidgets.addEventListener('navigateToChampionship', function (e) {
                if ('False'.toLocaleLowerCase() == 'true') {
                    let trnId = typeof e.data.tournamentId == 'undefined' ? e.data.Id : e.data.tournamentId;
                    location.href = "http://maxwin584.com/Sport/pre-match?" + trnId;
                } else {
                    let spId = typeof e.data.sportId == 'undefined' ? e.data.sport : e.data.sportId;
                    let champId = typeof e.data.championshipId == 'undefined' ? e.data.Id : e.data.championshipId;
                    location.href = "http://maxwin584.com/Sport/pre-match?" + cratePreMatchRouting(spId + "&" + e.data.country + "&" + champId);
                }
            });
            topWidgets.addEventListener('navigateToEvent', function (e) {
                if ('False'.toLocaleLowerCase() == 'true') {
                    let evId = typeof e.data.eventId == 'undefined' ? e.data.Id : e.data.eventId;
                    location.href = "http://maxwin584.com/Sport/event-details?" + evId;
                } else {
                    let spId = typeof e.data.sportId == 'undefined' ? e.data.sport : e.data.sportId;
                    let champId = typeof e.data.championshipId == 'undefined' ? e.data.champ : e.data.championshipId;
                    let evId = typeof e.data.eventId == 'undefined' ? e.data.Id : e.data.eventId;
                    location.href = "http://maxwin584.com/Sport/event-details?" + 'sport=' + spId + '&country=' + e.data.country + '&champ=' + champId + '&event=' + evId + '&live=' + (e.data.isLive ? "1" : "0") + '&supertip=0';
                }
            });
        });

        function cratePreMatchRouting(input) {
            let qs = '';
            const numbers = input.match(/\d+/g);
            if (numbers) {
                for (var i = 0; i < numbers.length; i++) {
                    if (i == 0) {
                        qs += 'sport=' + numbers[0];
                        continue;
                    } else if (i == 1) {
                        qs += '&country=' + numbers[1];
                        continue;
                    } else if (i == 2) {
                        qs += '&champ=' + numbers[2];
                        continue;
                    }
                    else {
                        qs += numbers[i];
                        if (i != numbers.length - 1) {
                            qs += '-';
                        }
                    }
                }
            }
            return qs;
        };
